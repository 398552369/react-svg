{
  "name": "react-svg",
  "version": "5.0.1",
  "description": "A React component that uses SVGInjector to add SVG to the DOM.",
  "main": "index.js",
  "module": "es/react-svg.js",
  "jsnext:main": "es/react-svg.js",
  "typings": "index.d.ts",
  "files": [
    "cjs",
    "es",
    "index.d.ts",
    "umd"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts,tsx}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:cjs": "rimraf cjs",
    "clean:coverage": "rimraf coverage",
    "clean:es": "rimraf es",
    "clean:umd": "rimraf umd",
    "compile": "tsc --emitDeclarationOnly",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "tslint -t stylish -p .",
    "postversion": "git push && git push --tags && npm publish",
    "release": "npm version -m 'Release v%s'",
    "test": "run-s check:* lint build test:*",
    "test:cjs": "jest --config ./scripts/jest/config.cjs.js",
    "test:cjsprod": "cross-env NODE_ENV=production jest --config ./scripts/jest/config.cjs.js",
    "test:es": "jest --config ./scripts/jest/config.es.js",
    "test:src": "jest --config ./scripts/jest/config.src.js",
    "test:umd": "jest --config ./scripts/jest/config.umd.js",
    "test:umdprod": "jest --config ./scripts/jest/config.umdprod.js",
    "version": "npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tanem/react-svg.git"
  },
  "keywords": [
    "javascript",
    "react",
    "svg",
    "svginjector"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "contributors": [
    "PrototypeAlex (https://github.com/PrototypeAlex)",
    "ChrisGarber-Medlever (https://github.com/ChrisGarber-Medlever)",
    "Ron Perris (https://github.com/ronperris)",
    "Jason Pettett (https://github.com/deadbeef404)",
    "Michael Robinson (https://github.com/faceleg)",
    "Thomas Groutars (https://github.com/tgroutars)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tanem/react-svg/issues"
  },
  "homepage": "https://github.com/tanem/react-svg",
  "peerDependencies": {
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^15.5.4 || ^16.0.0"
  },
  "dependencies": {
    "@tanem/svg-injector": "^1.2.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/enzyme": "^3.1.12",
    "@types/faker": "^4.1.2",
    "@types/jest": "^23.3.0",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/sinon": "^5.0.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "codecov": "^3.0.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "faker": "^4.1.0",
    "jest": "^23.4.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^4.0.0",
    "sinon": "^6.1.4",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  }
}
