{
  "name": "react-svg",
  "version": "4.0.6",
  "description": "A React component that uses SVGInjector to add SVG to the DOM.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:esm && npm run build:umd",
    "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel src -d lib",
    "build:esm": "babel src -d esm",
    "build:umd": "rollup -c",
    "clean": "rimraf coverage dist lib",
    "format": "prettier --write \"{examples,src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{examples,src,test}/**/*.js\"",
    "lint": "eslint .",
    "postversion": "git push && git push --tags && npm publish",
    "release": "npm version -m 'Release v%s'",
    "start": "cross-env BABEL_OUTPUT=commonjs babel-node ./scripts/webpack/start.js",
    "test": "npm run format:check && npm run lint && npm run clean && npm run build && npm run test:main && npm run test:module && npm run test:src && npm run test:umd && npm run test:umd:min",
    "test:main": "cross-env BABEL_OUTPUT=commonjs jest --config ./scripts/jest/config.main.js",
    "test:module": "cross-env BABEL_OUTPUT=commonjs jest --config ./scripts/jest/config.module.js",
    "test:src": "cross-env BABEL_OUTPUT=commonjs jest --config ./scripts/jest/config.src.js",
    "test:umd": "cross-env BABEL_OUTPUT=commonjs jest --config ./scripts/jest/config.umd.js",
    "test:umd:min": "cross-env BABEL_OUTPUT=commonjs jest --config ./scripts/jest/config.umd-min.js",
    "version": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tanem/react-svg.git"
  },
  "keywords": [
    "javascript",
    "react",
    "svg",
    "svginjector"
  ],
  "authors": [
    "PrototypeAlex (https://github.com/PrototypeAlex)",
    "Tane Morgan (https://github.com/tanem)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tanem/react-svg/issues"
  },
  "homepage": "https://github.com/tanem/react-svg",
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^15.5.4 || ^16.0.0"
  },
  "dependencies": {
    "@tanem/svg-injector": "^1.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.48",
    "@babel/core": "^7.0.0-beta.48",
    "@babel/node": "^7.0.0-beta.48",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.48",
    "@babel/preset-env": "^7.0.0-beta.48",
    "@babel/preset-react": "^7.0.0-beta.48",
    "babel-cli": "^7.0.0-beta.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "codecov": "^3.0.2",
    "cross-env": "^5.1.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^23.0.0",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.1.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "shortid": "^2.2.8",
    "sinon": "^5.0.10",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-dev-server": "^3.1.4"
  }
}
