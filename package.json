{
  "name": "react-svg",
  "version": "1.0.0",
  "description": "A react.js svg component for loading svg files into the dom.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist lib",
    "build:lib": "babel-node ./bin/build.js lib",
    "build:umd": "babel-node ./bin/build.js umd",
    "build:umd:min": "babel-node ./bin/build.js umd:min",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "lint": "eslint .",
    "prepublish": "in-publish && npm run clean && npm run build || not-in-publish",
    "test": "npm run lint && babel-node ./bin/test.js"
  },
  "repository": {

    "type": "git",
    "url": "https://github.com/atomic-app/react-svg"
  },
  "keywords": [
    "react",
    "react.js",
    "svg"
  ],
  "author": "PrototypeAlex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atomic-app/react-svg/issues"
  },
  "homepage": "https://github.com/atomic-app/react-svg",
  "peerDependencies": {
    "react": "^0.14.7"
  },
  "dependencies": {
    "svg-injector": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.2.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.14"
  }
}
