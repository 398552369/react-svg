{
  "name": "react-svg",
  "version": "2.2.0",
  "description": "A react.js svg component for loading svg files into the dom.",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf dist lib",
    "test": "jest && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true",
    "start": "babel-node ./bin/start.js",
    "build:lib": "babel src -d lib",
    "build:umd": "babel-node ./bin/build.js umd",
    "build:umd:min": "babel-node ./bin/build.js umd:min",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm publish",
    "release": "npm version -m 'Release v%s'"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomic-app/react-svg"
  },
  "keywords": [
    "react",
    "react.js",
    "svg"
  ],
  "author": "PrototypeAlex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atomic-app/react-svg/issues"
  },
  "homepage": "https://github.com/atomic-app/react-svg",
  "peerDependencies": {
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^15.5.4 || ^16.0.0"
  },
  "dependencies": {
    "svg-injector": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "eslint": "^4.7.2",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "husky": "0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "4.2.3",
    "prettier": "1.7.3",
    "prop-types": "^15.5.10",
    "raf": "^3.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^4.0.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/fixtures/",
      "<rootDir>/node_modules/",
      "<rootDir>/.jest-setup.js"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/.jest-setup.js"
    ],
    "roots": [
      "test"
    ]
  }
}
