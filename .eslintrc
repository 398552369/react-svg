{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", {
      "bracketSpacing": false,
      "jsxBracketSameLine": true,
      "singleQuote": true,
      "trailingComma": "all"
    }]
  }
}
